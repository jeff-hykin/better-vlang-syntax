#!/usr/bin/env bash

"$FORNIX_FOLDER/commands/project/build"

npx textmate-tester \
    --textmateExtension v \
    --syntax "$FORNIX_FOLDER/autogenerated/v.tmLanguage.json" \
    --examples "$FORNIX_FOLDER/language_examples/" \
    generate-specs --all
    # --supportSyntaxes '{ "source.v": "./autogenerated/v.tmLanguage.json" }' \